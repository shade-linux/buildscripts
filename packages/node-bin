# name: NodeJS
# pack: node-bin
# desc: Node.js is a JavaScript runtime built on Chrome's V8 JavaScript engine
# deps: none
# ver: 14.17.0

wget https://nodejs.org/dist/v14.17.0/node-v14.17.0-linux-x64.tar.xz
tar xzf node-v14.17.0-linux-x64.tar.xz
mv node-*/lib/* ${prefix}/lib
mv node-*/bin/* ${prefix}/bin
mv node-*/include/* ${prefix}/include
mv node-*/share/* ${prefix}/share
